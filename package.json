{
  "name": "numalauncherv2",
  "version": "0.0.0",
  "description": "沼でも使えるMinecraftランチャーV2",
  "license": "",
  "author": "",
  "main": "build/main.js",
  "scripts": {
    "dev": "run-p dev:*",
    "build": "cross-env NODE_ENV=\"production\" webpack --progress && node ./build.js",
    "predev": "rimraf build",
    "prepare": "husky install",
    "dev:webpack": "cross-env NODE_ENV=\"development\" webpack --progress",
    "dev:electron": "wait-on ./build/index.html ./build/main.js && electronmon ."
  },
  "lint-staged": {
    "src/**/*.ts(x)?": [
      "npx prettier",
      "npx eslint"
    ]
  },
  "electronmon": {
    "patterns": [
      "build/**/*"
    ]
  },
  "engines": {
    "node": "19.x.x"
  },
  "dependencies": {
    "adm-zip": "^0.5.10",
    "axios": "^1.2.6",
    "dotenv": "^16.0.3",
    "jotai": "^1.13.1",
    "node-fetch": "^3.3.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-transition-group": "^4.4.5",
    "skinview3d": "^3.0.0-alpha.1"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.1",
    "@types/adm-zip": "^0.5.0",
    "@types/node": "^18.11.18",
    "@types/react": "^18.0.25",
    "@types/react-dom": "^18.0.8",
    "@types/react-transition-group": "^4.4.5",
    "@typescript-eslint/eslint-plugin": "^5.49.0",
    "@typescript-eslint/parser": "^5.49.0",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "electron": "^22.1.0",
    "electron-builder": "^23.6.0",
    "electronmon": "^2.0.2",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-plugin-react": "^7.32.1",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^8.0.3",
    "jest": "^29.4.1",
    "lint-staged": "^13.1.0",
    "memfs": "^3.4.13",
    "mini-css-extract-plugin": "^2.6.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.8.3",
    "rimraf": "^4.1.2",
    "ts-jest": "^29.0.5",
    "ts-loader": "^9.4.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "wait-on": "^7.0.1",
    "webpack": "^5.75.0",
    "webpack-cli": "^5.0.1"
  },
  "build": {
    "appId": "com.example.electron-build-around",
    "files": [
      "build/**/*",
      "package.json"
    ],
    "directories": {
      "buildResources": "resources"
    },
    "publish": null
  }
}